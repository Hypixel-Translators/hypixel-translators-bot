{
	"name": "hypixel-translators",
	"version": "3.0.0",
	"private": true,
	"main": "dist/index",
	"license": "MIT",
	"scripts": {
		"start": "node --enable-source-maps --trace-warnings .",
		"pretty": "prettier --write src",
		"pretty:ci": "prettier --check src",
		"lint": "yarn pretty && eslint --fix src",
		"lint:ci": "yarn pretty:ci && eslint src",
		"compile": "rimraf dist/ && tsc -w",
		"postinstall": "tsc"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/Hypixel-Translators/hypixel-translators-bot.git"
	},
	"dependencies": {
		"@crowdin/crowdin-api-client": "^1.22.0",
		"@messageformat/core": "^3.0.1",
		"canvas": "^2.11.0",
		"discord.js": "^14.7.1",
		"language-flag-colors": "^2.1.2",
		"mongodb": "^5.1.0",
		"node-cron": "^3.0.2",
		"puppeteer": "18.2.1",
		"typescript": "^4.9.5"
	},
	"engines": {
		"node": "18.x"
	},
	"devDependencies": {
		"@types/node": "^18.14.6",
		"@types/node-cron": "^3.0.7",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.0",
		"dotenv": "^16.0.3",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-import-resolver-typescript": "^3.5.3",
		"eslint-plugin-import": "^2.27.5",
		"prettier": "^2.8.4",
		"rimraf": "^4.3.1"
	}
}
