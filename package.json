{
	"name": "hypixel-translators",
	"version": "3.0.0",
	"private": true,
	"main": "dist/index",
	"license": "MIT",
	"scripts": {
		"start": "node --enable-source-maps --trace-warnings .",
		"pretty": "prettier --write src",
		"pretty:ci": "prettier --check src",
		"lint": "yarn pretty && eslint --fix src",
		"lint:ci": "yarn pretty:ci && eslint src",
		"compile": "rimraf dist/ && tsc -w",
		"postinstall": "tsc"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/Hypixel-Translators/hypixel-translators-bot.git"
	},
	"dependencies": {
		"@crowdin/crowdin-api-client": "^1.18.2",
		"@messageformat/core": "^3.0.1",
		"canvas": "^2.9.2",
		"discord.js": "^14.0.0-dev.1655640259-358c3f4",
		"language-flag-colors": "^2.1.0",
		"mongodb": "^4.7.0",
		"node-cron": "^3.0.1",
		"puppeteer": "^15.1.0",
		"typescript": "^4.7.4",
		"uuid": "^8.3.2"
	},
	"engines": {
		"node": "18.x"
	},
	"devDependencies": {
		"@types/node": "^18.0.0",
		"@types/node-cron": "^3.0.1",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"dotenv": "^16.0.1",
		"eslint": "^8.18.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-no-one-time-vars": "^2.4.1",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2"
	}
}
